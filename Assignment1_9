//Program finds the area and perimeter of a rectangle of width 4.5 and height 7.9
public class Assignment1_9 {
	public static void main(String[] args) {
		System.out.print("The area is: ");
		System.out.println(4.5*7.9);
		System.out.print("The perimeter is: ");
		System.out.println((4.5*2)+(7.9*2));
	}
}
